mixin meta()
	meta(charset="utf-8")
	title #{title}
	meta(content=`${metaData[0].auth}` name="author")
	meta(content=`${metaData[0].desc}` name="description")
	meta(content=`${metaData[0].keyw}` name="keywords")
	meta(content="width=device-width, initial-scale=1" name="viewport")
	meta(content="ie=edge" http-equiv="x-ua-compatible")

	link(rel="shortcut icon" href="/assets/img/favicon.ico")

	//-link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css")

	link(rel="stylesheet" href=`/assets/css/foundation${suffix}.css${version}`)
	link(rel="stylesheet" href=`/assets/css/app${suffix}.css${version}`)

	script(src=`/assets/js/foundation${suffix}.js${version}` defer)
	script(src=`/assets/js/app${suffix}.js${version}` defer)

	<!--[if lt IE 9]>
	script(src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js")
	script(src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js")
	<![endif]-->
